{
  "name": "Invert.it",
  "description": "Go from Dark mode to Light mode, and vice versa",
  "version": "2.0",
  "manifest_version": 3,
  "background": {
    "scripts": [
      "popup.js"
    ],
    "persisten": true
  },
  "browser_action": {
    "default_title": "Invert.it",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tab"
  ]
}